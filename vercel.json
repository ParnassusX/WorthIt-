{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "bot/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "bot/bot.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ]
}